#!/bin/bash

modules="
unity.unity-debug
bungcip.better-toml
dbaeumer.vscode-eslint
eamodio.gitlens
groksrc.haml
tobiah.unity-tools
ms-python.python
vscode-icons-team.vscode-icons
rust-lang.rust
ahmadawais.shades-of-purple
mechatroner.rainbow-csv
"
for module in $modules; do
    code --install-extension "$module" || true
done
